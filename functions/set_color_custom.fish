function set_color_custom
    set -U fish_color_normal                normal
    set -U fish_color_command               0f0
    set -U fish_color_param                 090
    set -U fish_color_redirection           060
    set -U fish_color_comment               060
    set -U fish_color_error                 f00
    set -U fish_color_escape                0f0
    set -U fish_color_operator              0f0
    set -U fish_color_end                   0f0
    set -U fish_color_quote                 090
    set -U fish_color_autosuggestion        060
    set -U fish_color_valid_path            --underline
    set -U fish_color_cwd                   fff
    set -U fish_color_cwd_root              fff
    set -U fish_color_match                 fff
    set -U fish_color_search_match          --background=000
    set -U fish_color_selection             --background=000
    set -U fish_pager_color_prefix          090
    set -U fish_pager_color_completion      white 
    set -U fish_pager_color_description     white
    set -U fish_pager_color_progress        0f0
    set -U fish_color_history_current       0f0
end
