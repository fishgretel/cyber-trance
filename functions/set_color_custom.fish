function set_color_custom
    set -U fish_color_normal                normal
    set -U fish_color_command               66e
    set -U fish_color_param                 44a
    set -U fish_color_redirection           66a
    set -U fish_color_comment               66a
    set -U fish_color_error                 f00
    set -U fish_color_escape                66e
    set -U fish_color_operator              66e
    set -U fish_color_end                   66e
    set -U fish_color_quote                 44a
    set -U fish_color_autosuggestion        77b
    set -U fish_color_valid_path            --underline
    set -U fish_color_cwd                   eef
    set -U fish_color_cwd_root              eef
    set -U fish_color_match                 eef
    set -U fish_color_search_match          --background=002
    set -U fish_color_selection             --background=33e
    set -U fish_pager_color_prefix          44a
    set -U fish_pager_color_completion      77d
    set -U fish_pager_color_description     eef
    set -U fish_pager_color_progress        66c
    set -U fish_color_history_current       66c
end
