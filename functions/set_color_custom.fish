function set_color_custom
    set -U fish_color_normal                normal
    set -U fish_color_command               66e
    set -U fish_color_param                 44a
    set -U fish_color_redirection           669
    set -U fish_color_comment               669
    set -U fish_color_error                 f00
    set -U fish_color_escape                66e
    set -U fish_color_operator              66e
    set -U fish_color_end                   66e
    set -U fish_color_quote                 44a
    set -U fish_color_autosuggestion        669
    set -U fish_color_valid_path            --underline
    set -U fish_color_cwd                   fff
    set -U fish_color_cwd_root              fff
    set -U fish_color_match                 fff
    set -U fish_color_search_match          --background=000
    set -U fish_color_selection             --background=11e
    set -U fish_pager_color_prefix          44a
    set -U fish_pager_color_completion      66a
    set -U fish_pager_color_description     white
    set -U fish_pager_color_progress        55c
    set -U fish_color_history_current       55c
end
